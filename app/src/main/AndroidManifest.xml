        <activity android:name=".drawOther.weekendActivity" />
        <activity android:name=".drawOther.weekActivity" />
        <activity android:name=".drawOther.dayActivity" />
        <activity android:name=".guideActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>